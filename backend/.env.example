
# Environment Configuration Example for Kanbox Backend
NODE_ENV="development"

# Server Configuration
PORT=3000
CORS_ORIGIN="http://localhost:5173"

# Authentication Configuration
JWT_SECRET="your_jwt_secret_here"

# MongoDB Configuration
MONGO_USERNAME="your_mongo_username"
MONGO_PASSWORD="your_mongo_password"
MONGO_DATABASE="kanbox_development"
MONGODB_URI="mongodb://your_mongo_username:your_mongo_password@localhost:27017/kanbox_development?authSource=admin"

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME="your_cloudinary_cloud_name"
CLOUDINARY_API_KEY="your_cloudinary_api_key"
CLOUDINARY_API_SECRET="your_cloudinary_api_secret"
CLOUDINARY_URL="cloudinary://your_cloudinary_api_key:your_cloudinary_api_secret@your_cloudinary_cloud_name"
